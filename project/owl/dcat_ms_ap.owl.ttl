@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcat_ms_ap: <https://w3id.org/NFDI4Chem/dcat-MS-ap/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/NFDI4Chem/dcat-MS-ap.owl.ttl> a owl:Ontology ;
    rdfs:label "dcat-MS-ap" ;
    dcterms:license "MIT" ;
    dcterms:title "dcat-MS-ap" ;
    rdfs:seeAlso <https://NFDI4Chem.github.io/dcat-MS-ap> ;
    skos:definition "A dcat ap implementing the recommondations fo the MS MIChI." .

dcat_ms_ap:PersonCollection a owl:Class ;
    rdfs:label "PersonCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:entries ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat_ms_ap:Person ;
            owl:onProperty dcat_ms_ap:entries ] ;
    skos:definition "A holder for Person objects" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:NamedThing a owl:Class ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dcat_ms_ap:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dcat_ms_ap:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty dcat_ms_ap:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty dcat_ms_ap:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:id ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty dcat_ms_ap:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty dcat_ms_ap:age_in_years ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:vital_status ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty dcat_ms_ap:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat_ms_ap:PersonStatus ;
            owl:onProperty dcat_ms_ap:vital_status ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcat_ms_ap:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcat_ms_ap:vital_status ],
        dcat_ms_ap:NamedThing ;
    skos:definition "Represents a Person" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

PATO:0001421 a owl:Class ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf dcat_ms_ap:PersonStatus .

PATO:0001422 a owl:Class ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf dcat_ms_ap:PersonStatus .

<https://w3id.org/NFDI4Chem/dcat-MS-ap/PersonStatus#UNKNOWN> a owl:Class ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf dcat_ms_ap:PersonStatus .

dcat_ms_ap:entries a owl:ObjectProperty ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:age_in_years a owl:DatatypeProperty ;
    rdfs:label "age_in_years" ;
    rdfs:range xsd:integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:birth_date a owl:DatatypeProperty ;
    rdfs:label "birth_date" ;
    rdfs:range xsd:date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:description a owl:DatatypeProperty ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:range xsd:anyURI ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:primary_email a owl:DatatypeProperty ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:vital_status a owl:ObjectProperty ;
    rdfs:label "vital_status" ;
    rdfs:range dcat_ms_ap:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/NFDI4Chem/dcat-MS-ap> .

dcat_ms_ap:PersonStatus a owl:Class ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/NFDI4Chem/dcat-MS-ap/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/NFDI4Chem/dcat-MS-ap/PersonStatus#UNKNOWN> .


